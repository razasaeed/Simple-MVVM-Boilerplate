    <style name="CustomCardViewStyle" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_custom_corners</item>
    </style>
    <style name="RoundButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">@color/text_red</item>
        <item name="background">@drawable/ic_attention_cancel</item>
        <item name="backgroundColor">@color/text_red</item>
        <item name="android:backgroundTint">@color/text_red</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="RoundButtonStyleFilled" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">@color/white</item>
        <item name="background">@drawable/ic_attention_delete</item>
        <item name="backgroundColor">@color/text_red</item>
        <item name="android:backgroundTint">@color/text_red</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="ShapeAppearanceOverlay_card_custom_corners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">30dp</item>
        <item name="cornerSizeTopLeft">30dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>
    <style name="SheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackground">     @android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.3</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="SmallRoundButtonStyle" parent="TextAppearance.AppCompat.Button">

        <item name="android:textColor">@color/common_text_color</item>
        <item name="android:textSize">14sp</item>


        <item name="android:layout_height">40dp</item>
        <item name="cornerRadius">20dp</item>

        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>

    </style>
    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/background</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_icon</item>
        <item name="postSplashScreenTheme">@style/Theme.CloudStorage</item>

    </style>
    <style name="Theme.CloudStorage" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/maincolor</item>
        <item name="colorPrimaryVariant">@color/maincolor</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        
        <item name="windowNoTitle">true</item>
    </style>
    <style name="switchtheme" parent="Theme.AppCompat.Light">
        
        <item name="colorControlActivated">@color/maincolor</item>

        
        <item name="colorSwitchThumbNormal">#D9D9D9
        </item>

        
        <item name="android:colorForeground">#42221f1f
        </item>
    </style>
    <style name="wait">
        <item name="colorAccent">@color/maincolor</item>
